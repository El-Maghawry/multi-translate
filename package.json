{
  "name": "transee",
  "version": "1.2.3",
  "description": "Simple and useful tool for quick translation",
  "author": "Alessio Forte",
  "license": "MIT",
  "scripts": {
    "dev": "webpack --mode development --config config/webpack.dev.js && npm run start",
    "build": "webpack -p --mode production --config config/webpack.prod.js",
    "start": "electron ./src/main/main.dev.js",
    "watch": "webpack-dev-server --mode development --port 8182 --config config/webpack.dev.js",
    "delete:alljs": "rimraf dist/renderer/*.js dist/renderer/*.map",
    "delete:js": "rimraf dist/renderer/*.js",
    "delete:build": "rimraf dist/build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alessioforte/transee.git"
  },
  "homepage": "https://alessioforte.github.io/transee/",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "rxjs": "^5.5.2",
    "styled-components": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "devtron": "^1.4.0",
    "electron": "^1.8.2",
    "electron-devtools-installer": "^2.2.1",
    "electron-settings": "^3.1.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "file-loader": "^1.1.5",
    "rimraf": "^2.6.2",
    "style-loader": "^0.18.2",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.1"
  }
}
